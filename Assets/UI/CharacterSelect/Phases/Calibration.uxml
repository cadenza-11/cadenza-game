                <ui:Label text="X" name="update_MaxAttempts" />
            </ui:VisualElement>
            <ui:VisualElement name="c_Instructions" style="flex-grow: 1; flex-direction: row; justify-content: center;">
                <ui:Label text="Press" name="txt_Press1" />
                <ui:Label text="&lt;submit button&gt;" name="txt_TEMP_input" />
                <ui:Label text="to the beat!" name="txt_Press2" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
    <ui:VisualElement name="phase_Results" style="flex-grow: 1; display: none; align-self: center; justify-content: center;">
        <ui:Label text="Average latency:" name="txt_Results" />
        <ui:VisualElement name="c_Latency" style="flex-direction: row; justify-content: center;">
            <ui:Label text="0.0" name="update_Latency" />
